package superidei.com.gmail.com;
public class masivPoiskNechetnogo {

	public static void main(String[] args) {
		int [] arr1 = new int []{0,5,2,4,7,1,3,19};
		int kolvoNechetn =0;
		for (int i = 0; i< arr1.length; i++) {
			if (arr1[i]%2!=0) {
				kolvoNechetn++;
			}
		}System.out.println(" В данном массиве колличество нечетных цифр: "+kolvoNechetn);
		
	}

}
